{
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/prop-types": 0,
        "react/no-unused-state": 0,
        "jsx-a11y/no-static-element-interactions": 0,
        "jsx-a11y/anchor-is-valid": ["error", {
            "components": ["Link"],
            "specialLink": ["to"]
        }],
        "no-underscore-dangle": ["error", {
            "allow": ["__REDUX_DEVTOOLS_EXTENSION_COMPOSE__"]
        }]
    },
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    }
}