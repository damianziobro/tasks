{
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/prop-types": 0,
        "guard-for-in": 0,
        "react/no-unused-state": 0,
        "no-underscore-dangle": 0,
        "no-shadow": 0,
        "no-restricted-syntax": 0,
        "jsx-a11y/no-static-element-interactions": 0
    },
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
      }
}